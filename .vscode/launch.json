    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch App in development",
        "request": "launch",
        "type": "dart",
        "program": "flutter_app/lib/main_development.dart",
        "args": [
          "--flavor",
          "development",
          "--target",
          "lib/main_development.dart",
        ]
      },
      {
        "name": "Launch app in staging",
        "request": "launch",
        "type": "dart",
        "program": "flutter_app/lib/main_staging.dart",
        "args": [
          "--flavor",
          "staging",
          "--target",
          "lib/main_staging.dart"
        ]
      },
      {
        "name": "Launch app in production",
        "request": "launch",
        "type": "dart",
        "program": "flutter_app/lib/main_production.dart",
        "args": [
          "--flavor",
          "production",
          "--target",
          "lib/main_production.dart"
        ]
      }
    ]
  }